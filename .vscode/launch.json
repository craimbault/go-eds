    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "WebServ",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/webserv",
            "args": ["-key-string=12345678901234567890123456789012"],
            "cwd": "${workspaceFolder}"
        }
    ]
}